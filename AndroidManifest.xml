<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.knjc.animalproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/androidcat"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name="edu.knjc.animalproject.MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".information.InformationActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".inquiry.InquiryActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Classification.Classification" android:screenOrientation="portrait"></activity>
        <activity android:name=".Classification.Classification2" android:screenOrientation="portrait"></activity>
        <activity android:name=".email.PhotoEmail" android:screenOrientation="portrait"></activity>
        <activity android:name=".Classification.Classification3" android:screenOrientation="portrait"></activity>
        <activity android:screenOrientation="portrait" android:name=".information.InformationActivityWebView"></activity>
    </application>

</manifest>